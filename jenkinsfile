pipeline {
	agent any	tools {
		maven 'my-maven’
		jdk 'my-jdk’
	}
	stages {
		stage('Clone'){
			steps {git url:'https://github.com/Anjana-Natha/EurekaServer.git', branch:'main’}			}
		stage('Build'){
			steps {sh "mvn clean install -DskipTests"}
		}
		stage('Test'){
			steps{sh "mvn test"}		}
		}
		stage('Deploy') {
			steps { sh "docker build -t eureka-image ."
			            sh "docker run -p 8761:8761 -d --name eureka-sr eureka-image"}
		}
	}
}
